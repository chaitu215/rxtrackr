<h3>Note for <%= @note.medication.brand_name if @note.medication.brand_name? %>
      <%= @note.medication.generic_name if @note.medication.generic_name? %></h3>
<p><%= @note.content %></p>
<nav>
  <%= link_to "Edit", edit_note_path(@note), class: "btn btn-default" %>
  <%= link_to "Delete", note_path(@note), method: :delete, class: "btn btn-default" %>
  <%= link_to "Chart", user_medications_path(@user), class: "btn btn-default" %>
</nav>
