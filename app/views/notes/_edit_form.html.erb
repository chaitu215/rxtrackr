<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@note) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
      <div class="field">
        <%= f.label :content, "Notes" %> <br />
        <%= f.text_field :content, autofocus: true, class: 'form-control' %>
      </div>
      <div class="actions">
        <%= f.submit "Save", class: "btn btn-default" %>
      </div>
    <% end %>
  </div>
</div>
